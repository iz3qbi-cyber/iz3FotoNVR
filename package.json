{
  "name": "iz3fotonvr",
  "version": "2.0.0",
  "description": "iz3FotoNVR - Sistema completo di monitoraggio fotovoltaico Huawei SUN2000-10KTL-M1 con analisi pannello per pannello e DVR ONVIF multi-telecamera per Raspberry Pi",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "install-global": "npm install -g nodemon pm2",
    "pm2-start": "pm2 start server.js --name iz3fotonvr",
    "pm2-stop": "pm2 stop iz3fotonvr",
    "pm2-restart": "pm2 restart iz3fotonvr",
    "pm2-logs": "pm2 logs iz3fotonvr"
  },
  "dependencies": {
    "express": "^4.18.2",
    "node-onvif": "^0.1.4",
    "jsmodbus": "^4.0.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "raspberry-pi",
    "dvr",
    "nvr",
    "onvif",
    "cameras",
    "huawei",
    "solar",
    "photovoltaic",
    "modbus",
    "sun2000",
    "monitoring",
    "octopus",
    "energy",
    "panels",
    "optimizer",
    "iz3qbi"
  ],
  "author": {
    "name": "Giulio Zen",
    "email": "iz3qbi@gmail.com",
    "callsign": "IZ3QBI"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iz3qbi-cyber/iz3FotoNVR.git"
  },
  "homepage": "https://github.com/iz3qbi-cyber/iz3FotoNVR",
  "bugs": {
    "url": "https://github.com/iz3qbi-cyber/iz3FotoNVR/issues"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64",
    "arm"
  ]
}